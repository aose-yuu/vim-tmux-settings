        "statusBar.debuggingBackground": "#B4BE82",
        "statusBar.foreground": "#161821",
        "statusBar.debuggingForeground": "#161821"
    },
    "terminal.integrated.fontFamily": "Meslo LG M DZ for Powerline",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.colorTheme": "One Monokai",
    "files.autoSave": "afterDelay",
    "emmet.includeLanguages": {
        "liquid": "html",
        "erb": "html",
    },
    "editor.cursorBlinking": "smooth",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.letterSpacing": 1,
    "background.opacity": 0.87,
    "code-runner.runInTerminal": true,
    "editor.bracketPairColorization.enabled": true,

    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.darwin": "/Users/komachi/.config/nvim/init.vim",
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },

    "window.zoomLevel": 1,
    "background.filepath": "/Users/komachi/Desktop/NCG287.jpeg"
